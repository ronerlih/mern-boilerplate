{
  "name": "mern",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "PUBLIC_URL": "https://mern-stack-boilerplat.herokuapp.com/",
  "home": "https://mern-stack-boilerplat.herokuapp.com/",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node index.js prod ",
    "start:prod-no-cluster": "node index.js no-cluster prod",
    "start:dev": "concurrently \"nodemon index --ignore 'client/*'\" \"npm run client\"",
    "start:dev-no-cluster": "concurrently \"nodemon index no-cluster --ignore 'client/*'\" \"npm run client\"",
    "start:dev-no-ssr": "concurrently \"nodemon index no-ssr --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.3",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.8",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "ignore-styles": "^5.0.1",
    "ioredis": "^4.17.3",
    "js-cookie": "^2.2.1",
    "memorystore": "^1.6.2",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redis": "^2.8.0",
    "session": "^0.1.0"
  }
}
